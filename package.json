{
  "name": "ibt-telemetry",
  "version": "1.1.0",
  "description": "iRacing ibt parser",
  "keywords": [
    "iracing",
    "irsdk",
    "ibt",
    "telemetry"
  ],
  "author": "Steven Inskip",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SkippyZA/ibt-telemetry.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "npm run lint && tsc -p tsconfig.build.json",
    "lint": "eslint '**/*.{js,ts}'",
    "test:dependencies": "depcruise --validate .dependency-cruiser.js src",
    "prepublish": "npm run compile",
    "postinstall": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.335.0",
    "@aws-sdk/types": "^3.329.0",
    "@smithy/types": "^1.0.0",
    "js-yaml": "3.13.1",
    "ramda": "0.26.1",
    "rxjs": "6.6.7"
  },
  "devDependencies": {
    "@types/js-yaml": "3.12.1",
    "@types/node": "^12.12.21",
    "@types/ramda": "0.26.38",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "dependency-cruiser": "6.0.0",
    "eslint": "^8.43.0",
    "typescript": "^5.0.4"
  }
}
